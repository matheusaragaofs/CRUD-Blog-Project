<%- include('../../partials/header')  %> 

<%- include('../../partials/navbar')  %> 

<div class="container">
    <h2 class='display-4'>Usuários</h2>
    <a style='margin-bottom: 10px;'class='btn btn-primary'href="/admin/users/create">Criar um novo usuário</a>
        <table class='table '>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
               
                    <% users.forEach(user => { %>
                        <tr>
                            <th><%=user.id  %>  </th>
                     <th >  <%=user.email %> </th>
                    </tr>
                    <% }) %>
              
            </tbody>

        </table>

</div>


<%- include('../../partials/footer')  %> 
 
